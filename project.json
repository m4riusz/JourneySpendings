{
	"include": [
		"packages.json",
		"templates.json"
	],
	"name": "JourneySpendings",
	"configs": {
		"Dev": "debug",
		"Prod": "release"
	},
	"configFiles": {
		"Dev": "Settings/Dev.xcconfig",
		"Prod": "Settings/Prod.xcconfig"
	},
	"options": {
		"bundleIdPrefix": "pl.msut",
		"deploymentTarget": {
			"iOS": "13.0"
		},
		"createIntermediateGroups": true,
		"generateEmptyDirectories": true
	},
	"targets": {
		"About": {
			"templates": [
				"BaseFramework"
			],
			"dependencies": [
				{
					"target": "Core"
				}
			]
		},
		"AboutTests": {
			"templates": [
				"BaseFrameworkUnitTest"
			],
			"templateAttributes": {
				"tests_parent": "About"
			}
		},
		"Core": {
			"templates": [
				"BaseFramework"
			]
		},
		"CoreTests": {
			"templates": [
				"BaseFrameworkUnitTest"
			],
			"templateAttributes": {
				"tests_parent": "Core"
			}
		},
		"Journey": {
			"templates": [
				"BaseFramework"
			],
			"dependencies": [
				{
					"target": "Core"
				}
			]
		},
		"JourneyTests": {
			"templates": [
				"BaseFrameworkUnitTest"
			],
			"templateAttributes": {
				"tests_parent": "Journey"
			}
		},
		"MobileApp": {
			"type": "application",
			"templates": [
				"BaseTarget"
			],
			"dependencies": [
				{
					"target": "About"
				},
				{
					"target": "Core"
				},
				{
					"target": "Journey"
				}
			],
			"preBuildScripts": [
				{
					"name": "Swiftlint",
					"path": "Scripts/swiftlint.sh"
				}
			]
		},
		"MobileAppTests": {
			"templates": [
				"BaseUnitTest"
			],
			"templateAttributes": {
				"tests_parent": "MobileApp"
			}
		},
		"MobileAppUITests": {
			"type": "bundle.ui-testing",
			"platform": "iOS",
			"sources": [
				{
					"path": "MobileAppUITests/Sources"
				},
				{
					"path": "MobileAppUITests/Info.plist"
				}
			],
			"dependencies": [
				{
					"target": "MobileApp"
				}
			],
			"settings": {
				"TEST_TARGET_NAME": "MobileApp"
			}
		}
	},
	"schemes": {
		"MobileApp": {
			"build": {
				"targets": {
					"MobileApp": "all"
				}
			},
			"test": {
				"gatherCoverageData": true,
				"language": "pl",
				"region": "PL",
				"coverageTargets": [
					"Core",
					"About",
					"Journey",
					"MobileApp"
				],
				"targets": [
					"CoreTests",
					"AboutTests",
					"JourneyTests",
					"MobileAppTests"
				]
			}
		},
		"MobileAppUITests": {
			"build": {
				"targets": {
					"MobileAppUITests": [
						"run",
						"test"
					]
				}
			},
			"test": {
				"targets": [
					"MobileAppUITests"
				]
			}
		}
	}
}