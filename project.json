{
    "name":"MobileApp",
    "options":{
       "bundleIdPrefix":"pl.msut",
       "deploymentTarget":{
          "iOS":"13.0"
       },
       "createIntermediateGroups":true
    },
    "packages":{
       "Swinject":{
          "url":"https://github.com/Swinject/Swinject.git",
          "from":"2.8.0"
       },
       "SnapKit":{
          "url":"https://github.com/SnapKit/SnapKit.git",
          "from":"5.0.1"
       },
       "RxSwift":{
          "url":"https://github.com/ReactiveX/RxSwift.git",
          "from":"6.2.0"
       },
       "RxCocoa":{
          "url":"https://github.com/ReactiveX/RxSwift.git",
          "from":"6.2.0"
       }
    },
    "targets":{
       "Core":{
          "type":"framework",
          "platform":"iOS",
          "sources":[
             {
                "path":"Core/Info.plist"
             },
             {
                "path":"Core/Sources"
             },
             {
                "path":"Core/Resources"
             }
          ],
          "dependencies":[
             {
                "package":"Swinject"
             }
          ]
       },
       "JourneySpendings":{
          "type":"application",
          "platform":"iOS",
          "sources":[
             {
                "path":"JourneySpendings/Sources"
             },
             {
                "path":"JourneySpendings/Resources"
             },
             {
                "path":"JourneySpendings/Info.plist"
             }
          ],
          "dependencies":[
             {
                "target":"Core"
             },
             {
                "package":"SnapKit"
             },
             {
                "package":"RxSwift"
             },
             {
                "package":"RxCocoa"
             },
             {
                "package":"Swinject"
             }
          ]
       }
    }
 }